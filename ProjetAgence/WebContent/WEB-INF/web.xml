  </servlet>
  <servlet>
    <servlet-name>GestionOffre</servlet-name>
    <servlet-class>servlet.GestionOffre</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>servlet.Login</servlet-class>
  </servlet>
  <servlet>
